        <action
            android:id="@+id/action_reviewAllData_to_addressDetails"
            app:destination="@id/addressDetails" />
        <action
            android:id="@+id/action_reviewAllData_to_basicdetails3"
            app:destination="@id/basicdetails3" />
        <action
            android:id="@+id/action_reviewAllData_to_basicdetails1"
