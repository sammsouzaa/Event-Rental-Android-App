        <action
            android:id="@+id/action_basicdetails3_to_viewMyLocations"
            app:destination="@id/viewMyLocations" />
        <action
            android:id="@+id/action_basicdetails3_to_basicdetails2"
            app:destination="@id/basicdetails2" />
        <action
            android:id="@+id/action_basicdetails3_to_reviewAllData"
            app:destination="@id/reviewAllData" />
